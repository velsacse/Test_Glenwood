			<custom:FastButton normalStyle='{resources.tabletstyles.menuNormal}'
				holdPressStyle='{resources.tabletstyles.menuHoldPress}' text="Refresh"
				ui:field="Refresh" />
			
			<custom:FastButton normalStyle='{resources.tabletstyles.menuNormal}'
