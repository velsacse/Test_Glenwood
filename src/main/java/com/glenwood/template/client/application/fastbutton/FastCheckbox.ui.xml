<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources"
		type="com.glenwood.template.client.resource.TabletResources" />
	<g:HorizontalPanel styleName="{resources.fastCheckboxStyles.container}"
		verticalAlignment="ALIGN_MIDDLE">
		<g:HTMLPanel styleName="{resources.fastCheckboxStyles.unchecked}" ui:field="checkbox" />
		<g:InlineLabel styleName="{resources.fastCheckboxStyles.text}" ui:field="text"
			text="" />
	</g:HorizontalPanel>



</ui:UiBinder>